
public class ClassMain {

	public static void main(String[] args) {
		Ponto pontoA = new Ponto(7, 9);
		Ponto pontoB = new Ponto(0, 4);

		boolean Igualdade = pontoA.SãoIguais(5,3);
		System.out.println(Igualdade);
		
		boolean Igualdade2 = pontoA.SãoIguais2(pontoB);
		System.out.println(" O pontoA e o pontoB tem valores iguals ? " + Igualdade2);

	}
}
